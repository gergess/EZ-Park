<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_profile">

    <fragment
        android:id="@+id/navigation_receipt"
        android:name="com.ez.ez_park.ui.ReceiptFragment"
        android:label="@string/title_sign_up"
        tools:layout="@layout/fragment_receipt" />

    <fragment
        android:id="@+id/navigation_map"
        android:name="com.ez.ez_park.ui.MapsFragment"
        android:label="@string/title_sign_up"
        tools:layout="@layout/activity_maps_fragment" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.ez.ez_park.ui.ProfileFragment"
        android:label="@string/title_sign_up"
        tools:layout="@layout/fragment_profile" />

</navigation>